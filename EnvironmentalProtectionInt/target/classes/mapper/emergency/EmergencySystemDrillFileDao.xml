<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.tengdi.environmentalprotectionint.modules.emergency.dao.EmergencySystemDrillFileDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.tengdi.environmentalprotectionint.modules.emergency.entity.EmergencySystemDrillFileEntity" id="emergencySystemDrillFileMap">
        <result property="pid" column="pid"/>
        <result property="cid" column="cid"/>
        <result property="aid" column="aid"/>
        <result property="fileName" column="file_name"/>
        <result property="attachmentSize" column="attachment_size"/>
        <result property="attachmentFormat" column="attachment_format"/>
        <result property="attachmentUrl" column="attachment_url"/>
        <result property="uploadPerson" column="upload_person"/>
        <result property="uploadTime" column="upload_time"/>
        <result property="createdate" column="createdate"/>
    </resultMap>

    <select id="dataByFile" parameterType="java.lang.String" resultType="com.tengdi.environmentalprotectionint.modules.emergency.entity.EmergencySystemDrillFileEntity">
        SELECT * FROM emergency_system_drill_file WHERE aid=#{aid}
    </select>

</mapper>